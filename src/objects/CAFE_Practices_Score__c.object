        <formula>IF( Farm__r.permWorkersMinimumMonthWage__c &gt;= 644.350, 1, 0)+ IF( Farm__r.tempWorkersMinimumDailyWage__c &gt;= (644.350/30), 1, 0) +  IF( TEXT(Farm__r.workersPaidRegularly__c) = &apos;Cumple&apos;, 1, 0)+  IF( INCLUDES(Farm__r.forcedLabor__c, &quot;No hay trabajo forzado, No hay prácticas de retención, No hay empleados ilegales, No hay empleados ilegales, No se les retiene su identificación, No pagan depósitos para poder trabajar&quot;) , 1, 0) +  IF(INCLUDES( Farm__r.hiresUnderAge__c, &quot;Sí, de menos de 14 años&quot;), 1, 0) +  IF(INCLUDES( Farm__r.underAgeWorkConditions__c, &quot;Van a la escuela, No sobrepasan las horas de trabajo, No ponen en riesgo salud y crecimiento&quot;), 1, 0) +  IF(TEXT( Farm__r.discrimination__c) = &apos;De acuerdo&apos;, 1, 0) + IF(TEXT(Farm__r.physicalOralSexualAbuse__c)= &apos;En desacuerdo&apos;, 1, 0) +  IF(TEXT( Baseline__r.socioe_childrenStudying__c )= &apos;Si, TODOS están asistiendo&apos;, 1, 0) +  IF(TEXT( Farm__r.useOfProtApplicationDangSubstances__c)= &apos;Sí, siempre&apos;, 1, 0) +  IF(TEXT( Farm__r.underagePregnantProtected__c)= &apos;Sí se cumple&apos;, 1, 0) +  IF(TEXT( Farm__r.dangSubstancesManipulatorsTrained__c)= &apos;Sí&apos;, 1, 0) +  IF(TEXT( Farm__r.entranceProhibited48Hours__c)= &apos;Cumple&apos;, 1, 0)</formula>
