        <fullName>Coffee_Growing_Practices_Met__c</fullName>
        <externalId>false</externalId>
        <formula>IF(INCLUDES(Farm__r.waterBodiesBufferZones__c , &quot;&gt; al 50% de cuerpos de agua permanentes&quot;) || INCLUDES(Farm__r.waterBodiesBufferZones__c , &quot;100% de cuerpos de agua permanentes&quot;) , 1, 0)+ IF(INCLUDES(Farm__r.waterBodiesBufferZones__c , &quot;&gt; al 50% de cuerpos de agua intemitentes&quot;) || INCLUDES(Farm__r.waterBodiesBufferZones__c , &quot;100% de cuerpos de agua intermitentes&quot;), 1, 0)+ IF(TEXT( Farm__r.wasteDisposedSafeWay__c ) = &apos;Cumple&apos;, 1, 0)+ IF(TEXT(Farm__r.erosionZonesIdentified__c)= &apos;Sí&apos;, 1, 0) + IF(INCLUDES( Farm__r.farmManagmtDocuments__c, &quot;Plan de conservación de suelos&quot; ), 1, 0)+ IF(Farm__r.percErosionZoneRiskPrevention__c &gt;= 0.5, 2, 0)+ IF(Farm__r.percErosionZoneRiskPrevention__c == 100, 1, 0)+ IF(INCLUDES( Farm__r.weedsControlMethods__c,&quot;Control físico/mecánico&quot;) || INCLUDES( Farm__r.weedsControlMethods__c,&quot;Cultivos de cobertura&quot; ) || INCLUDES( Farm__r.weedsControlMethods__c,&quot;Mulch (colchón material orgánico)&quot; ) || INCLUDES( Farm__r.weedsControlMethods__c,&quot;Distancias de siembra&quot; ) || INCLUDES( Farm__r.weedsControlMethods__c,&quot;Manejo de Sombra&quot; ), 1, 0)+ IF( Farm__r.percShadeLeguminous__c &gt;= 0.25, 2, 0)+ IF(TEXT( Farm__r.lastSoilSampling__c )= &apos;entre 1-2 años&apos; || TEXT( Farm__r.lastSoilSampling__c )= &apos;menos de 1 año&apos; , 1, 0)+ IF(TEXT( Farm__r.fertAcordingSoilAnalysis__c)= &apos;Fertiliza basado en análisis de suelo&apos;, 1, 0)+ IF(INCLUDES( Farm__r.farmManagmtDocuments__c, &quot;Plan de manejo de sombra&quot;), 1, 0)+ IF( Farm__r.shadowLevelsNumber__c = 1 &amp;&amp; Farm__r.percentageShade__c &gt;= 0.1, 1, 0)+ IF( Farm__r.numberOfShadeSpecies__c &gt;= 2, 1, 0)+ IF( Farm__r.shadowLevelsNumber__c = 1 &amp;&amp; Farm__r.percentageShade__c &gt;= 0.4 || Farm__r.shadowLevelsNumber__c &gt;= 2 , 1, 0)+ IF(TEXT( Farm__r.promotesFloraAndFaunaPreservation__c)= &apos;Si se cumple&apos;, 1, 0)+ IF(TEXT( Farm__r.turnedForestInCropArea__c) &lt;&gt; &apos;Sí&apos;, 1, 0)+ IF(TEXT( Farm__r.biologicalDiversityContrib__c)= &apos;Sí&apos;, 1, 0)+ IF(INCLUDES( Farm__r.diseaseControlAgrochemicals__c,&quot;No usa ningún producto químico&quot;) &amp;&amp; INCLUDES( Farm__r.weedsControlHerbicides__c , &quot; &quot;) &amp;&amp; INCLUDES( Farm__r.plagueControlAgroChemicals__c , &quot;No usa ningún producto químico&quot;), 1, 0)+ IF(INCLUDES( Farm__r.farmManagmtDocuments__c , &quot;Registro de insumos; Plan de MIPE&quot;), 1, 0)+ IF(TEXT( Farm__r.safeStorageForSupplies__c ) = &apos;Cumple&apos;, 2, 0)+ IF(INCLUDES( Farm__r.agrochemOriginalContainerProperUse__c , &quot;Cumple&quot;), 1, 0)+ IF(INCLUDES( Farm__r.farmManagmtDocuments__c , &quot;Plan de MIPE&quot;), 1, 0)+ IF(INCLUDES( Farm__r.registersPlague_DiseaseCtrlSupplies__c , &quot;Fecha de aplicación; Dosis; Lote&quot;), 1, 0)+ IF(TEXT( Farm__r.containersNotReused__c) = &apos;Cumple&apos;, 1, 0)+ IF(INCLUDES( Farm__r.farmManagmtDocuments__c , &quot;Plan y registro de Podas de café&quot;), 1, 0)+ IF(IF( Farm__r.Farm_area_Ha__c &gt;0&amp;&amp; Farm__r.Forest_Area_Ha__c&gt;0,Farm__r.Farm_area_Ha__c/ Farm__r.Forest_Area_Ha__c,0 )&gt;= 0.05 , 1, 0)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
