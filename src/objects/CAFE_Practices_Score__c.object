        <formula>IF(TEXT( Farm__r.wetmill_lastWashWaterRecycled__c) = &apos;Sí&apos;, 1, 0) +  IF(TEXT( Farm__r.wetmill_riskOfContamWResidualWater__c )= &apos;No&apos;, 1, 0) +  IF(TEXT( Farm__r.wetmill_residualWaterDistOver40__c ) = &apos;Sí&apos;, 1, 0) +  IF(INCLUDES( Farm__r.wetmill_pulpManagement__c ,&quot;La amontona sin protección&quot;) || INCLUDES( Farm__r.wetmill_pulpManagement__c ,&quot;Lo vierte al río&quot;) || INCLUDES( Farm__r.wetmill_pulpManagement__c ,&quot;La bota&quot;) || INCLUDES( Farm__r.wetmill_pulpManagement__c ,&quot;Riesgo de contaminar fuentes de agua&quot;) || INCLUDES( Farm__r.wetmill_pulpManagement__c ,&quot;Otros&quot;), 0, 1) + IF(INCLUDES( Farm__r.wetmill_pulpManagement__c ,&quot;Procesa en compost en cuanto es posible&quot;), 1, 0) +  IF(INCLUDES( Farm__r.wetmill_dryMeans__c , &quot;Secadora mecánica&quot;) || INCLUDES( Farm__r.wetmill_dryMeans__c , &quot;Otro&quot;), 0, 1) +  IF(TEXT( Farm__r.wetmill_documentationAndLabeling__c ) = &apos;Sí&apos;, 1, 0)</formula>
