<?xml version="1.0" encoding="UTF-8"?>
<CustomObject xmlns="http://soap.sforce.com/2006/04/metadata">
    <deploymentStatus>Deployed</deploymentStatus>
    <description>Summary of performance of each CKW in a given week or month, per different targets</description>
    <enableActivities>true</enableActivities>
    <enableEnhancedLookup>true</enableEnhancedLookup>
    <enableHistory>true</enableHistory>
    <enableReports>true</enableReports>
    <fields>
        <fullName>Actual_Airtime__c</fullName>
        <description>The airtime was actually credited to the user&apos;s account, based on our performance calculations. &quot;Airtime Date&quot; should be set at the same time.</description>
        <label>Actual Airtime</label>
        <precision>12</precision>
        <scale>2</scale>
        <type>Number</type>
    </fields>
    <fields>
        <fullName>Actual_Payment__c</fullName>
        <description>The actual payment that was made to the CKW; this field should be filled after the payment is processed (Payment Date should be filled at the same time).</description>
        <label>Actual Payment</label>
        <precision>12</precision>
        <scale>2</scale>
        <type>Number</type>
    </fields>
    <fields>
        <fullName>Airtime_Date__c</fullName>
        <description>The date that airtime was actually credited to the user&apos;s account. This should be updated when &quot;Actual Airtime&quot; set.</description>
        <label>Airtime Date</label>
        <type>DateTime</type>
    </fields>
    <fields>
        <fullName>CKW_c__c</fullName>
