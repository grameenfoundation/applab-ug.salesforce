    <fields>
        <fullName>Puntaje_Recuperacion_de_zonas_erosionada__c</fullName>
        <formula>IF(  OR( 
INCLUDES( Recuperacion_de_zonas_erosionadas__c , &apos;Curvas a nivel&apos;),
INCLUDES( Recuperacion_de_zonas_erosionadas__c , &apos;Terrazas individuales&apos;),
INCLUDES( Recuperacion_de_zonas_erosionadas__c , &apos;Terrazas de banco&apos;),
