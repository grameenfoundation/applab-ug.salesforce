INCLUDES( Recuperacion_de_zonas_erosionadas__c , &apos;Barreras físicas&apos;),
INCLUDES( Recuperacion_de_zonas_erosionadas__c , &apos;Acequias&apos;),
INCLUDES( Recuperacion_de_zonas_erosionadas__c , &apos;Cultivos de cobertura de suelo&apos;),
INCLUDES( Recuperacion_de_zonas_erosionadas__c , &apos;Mulch (colchon de materia orgánica)&apos;)
)  , 1, 0)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
