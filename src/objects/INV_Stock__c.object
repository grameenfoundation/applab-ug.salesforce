        <label>Article Name</label>
        <type>Text</type>
    </fields>
    <fields>
        <fullName>Article__c</fullName>
        <label>Article</label>
        <referenceTo>INV_Article__c</referenceTo>
        <relationshipLabel>INV_Stock_Article</relationshipLabel>
        <relationshipName>INV_Stock_Article</relationshipName>
        <required>true</required>
        <type>Lookup</type>
    </fields>
    <fields>
        <fullName>Client_Name__c</fullName>
        <formula>Client__r.Description__c  
/*
Client__r.Contact__r.FirstName  +&quot; &quot;+   Client__r.Contact__r.LastName
*/</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Client Name</label>
        <type>Text</type>
    </fields>
    <fields>
        <fullName>Client__c</fullName>
        <label>Client</label>
