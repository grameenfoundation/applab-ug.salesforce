    <validationRules>
        <fullName>One_phone_number</fullName>
        <active>true</active>
        <description>Enforces the rule that only one of Handset or Raw Mobile Number is allowed to be set</description>
        <errorConditionFormula>NOT( ISBLANK( Handset__c ))  &amp;&amp; (LEN( Raw_Mobile_Number__c ) &gt;0)</errorConditionFormula>
        <errorMessage>Either Handset or Raw Mobile Number can be set, not both</errorMessage>
    </validationRules>
