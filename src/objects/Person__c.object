        <label>Education Level</label>
        <picklist>
            <picklistValues>
                <fullName>Primary Education</fullName>
                <default>false</default>
            </picklistValues>
            <picklistValues>
                <fullName>Graduate</fullName>
                <default>false</default>
            </picklistValues>
            <picklistValues>
                <fullName>O-Level</fullName>
                <default>false</default>
            </picklistValues>
            <picklistValues>
                <fullName>A-Level</fullName>
                <default>false</default>
            </picklistValues>
            <picklistValues>
                <fullName>Certificate</fullName>
                <default>false</default>
            </picklistValues>
            <picklistValues>
                <fullName>Diploma</fullName>
                <default>false</default>
            </picklistValues>
            <picklistValues>
                <fullName>No Grade Completed</fullName>
                <default>false</default>
            </picklistValues>
            <sorted>false</sorted>
        </picklist>
        <type>Picklist</type>
    </fields>
    <fields>
        <fullName>Email_Address__c</fullName>
        <description>The email address to contact this person by</description>
