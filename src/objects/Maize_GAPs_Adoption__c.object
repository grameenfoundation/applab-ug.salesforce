        <formula>IF 	(  OR(AND (TEXT(Ability_to_detect_pests_disease__c)==&apos;Yes&apos;, OR (TEXT(Pest_Disease_Control_Measures__c)==&apos;Spray with chemicals (pesticides)&apos;,
