															)
		),
		AND (TEXT(Ability_to_detect_pests_disease__c)==&apos;Yes&apos;, 
			TEXT(Pest_Disease_Control_Measures__c)==&apos;Spray with chemicals (pesticides)&apos;, 
			OR(TEXT(Frequency_of_Pesticide_Use__c)==&apos;Once&apos;, TEXT(Frequency_of_Pesticide_Use__c)==&apos;Twice&apos;)
		)
	),
