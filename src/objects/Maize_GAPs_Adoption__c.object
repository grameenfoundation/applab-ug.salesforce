		 			        Text(Stage_of_Thinning__c)==&apos;Three weeks after germination &apos;
 						)
			   )
			)
		, 
		AND (TEXT(Cropping_System_Intercrop__c)==&apos;No&apos;, TEXT(Spacing_Measurement__c)==&apos;Use strings or ropes&apos;, 
			 OR(
			 		AND ( Text(Spacing_inter_crop__c)==&apos;2.5ft by 1ft &apos;, Text(Seed_rate__c)==&apos;One seed per hole&apos;
			 			),
		 			AND ( Text(Spacing_inter_crop__c)==&apos;2.5ft by 1ft &apos;, OR(Text(Seed_rate__c)==&apos;Two seeds per hole&apos;, Text(Seed_rate__c)==&apos;Three seeds per hole&apos;), 
