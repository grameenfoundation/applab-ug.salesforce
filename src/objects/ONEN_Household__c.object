<?xml version="1.0" encoding="UTF-8"?>
<CustomObject xmlns="http://soap.sforce.com/2006/04/metadata">
    <deploymentStatus>Deployed</deploymentStatus>
    <description>Physical mailing address for grouping contacts</description>
    <enableActivities>true</enableActivities>
    <enableEnhancedLookup>true</enableEnhancedLookup>
    <enableReports>true</enableReports>
    <fields>
        <fullName>Anonymous__c</fullName>
        <defaultValue>false</defaultValue>
        <description>Donor wants to be anonymous</description>
        <label>Anonymous</label>
        <type>Checkbox</type>
    </fields>
    <fields>
        <fullName>Household_Maintenance__c</fullName>
        <formula>HYPERLINK(&quot;/servlet/servlet.Integration?lid=01N700000000oxV&amp;eid=&quot; &amp; Id  &amp; &quot;&amp;direction=toContact&quot;, IMAGE( &quot;servlet/servlet.FileDownload?file=01570000000LVBk&quot;, &quot;image from Documents tab&quot;))</formula>
        <label>Household Maintenance</label>
        <type>Text</type>
    </fields>
    <fields>
        <fullName>MailingCity__c</fullName>
        <label>Mailing City</label>
        <length>255</length>
        <type>Text</type>
    </fields>
    <fields>
        <fullName>MailingCountry__c</fullName>
        <label>Mailing Country</label>
        <length>255</length>
        <type>Text</type>
    </fields>
    <fields>
        <fullName>MailingPostalCode__c</fullName>
        <label>Mailing Zip/Postal Code</label>
        <length>10</length>
        <type>Text</type>
    </fields>
    <fields>
        <fullName>MailingState__c</fullName>
        <label>Mailing State/Province</label>
        <length>255</length>
        <type>Text</type>
    </fields>
    <fields>
        <fullName>MailingStreet__c</fullName>
        <label>Mailing Street</label>
        <type>TextArea</type>
    </fields>
    <fields>
        <fullName>Recognition_Name_Short__c</fullName>
        <description>Short version of prefered</description>
        <label>Recognition Name - Short</label>
        <length>255</length>
        <type>Text</type>
    </fields>
    <fields>
        <fullName>Recognition_Name__c</fullName>
        <description>Preferred name when recognizing gifts</description>
        <label>Recognition Name</label>
        <length>255</length>
        <type>Text</type>
    </fields>
    <fields>
        <fullName>household_id__c</fullName>
        <description>for migration</description>
        <label>household id</label>
        <precision>7</precision>
        <scale>0</scale>
        <type>Number</type>
    </fields>
    <label>Household</label>
    <listViews>
        <fullName>All</fullName>
        <filterScope>Everything</filterScope>
        <label>All</label>
    </listViews>
    <nameField>
        <label>Household Name</label>
        <type>Text</type>
    </nameField>
    <pluralLabel>Households</pluralLabel>
    <searchLayouts>
        <customTabListAdditionalFields>MailingStreet__c</customTabListAdditionalFields>
        <customTabListAdditionalFields>MailingCity__c</customTabListAdditionalFields>
        <customTabListAdditionalFields>MailingState__c</customTabListAdditionalFields>
        <customTabListAdditionalFields>MailingPostalCode__c</customTabListAdditionalFields>
        <lookupDialogsAdditionalFields>MailingCity__c</lookupDialogsAdditionalFields>
        <lookupDialogsAdditionalFields>MailingState__c</lookupDialogsAdditionalFields>
        <lookupDialogsAdditionalFields>MailingStreet__c</lookupDialogsAdditionalFields>
        <lookupDialogsAdditionalFields>MailingPostalCode__c</lookupDialogsAdditionalFields>
        <searchResultsAdditionalFields>MailingStreet__c</searchResultsAdditionalFields>
        <searchResultsAdditionalFields>MailingCity__c</searchResultsAdditionalFields>
        <searchResultsAdditionalFields>MailingState__c</searchResultsAdditionalFields>
        <searchResultsAdditionalFields>MailingPostalCode__c</searchResultsAdditionalFields>
    </searchLayouts>
    <sharingModel>Private</sharingModel>
</CustomObject>
