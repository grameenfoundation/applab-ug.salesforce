        <formula>((IF(TEXT(LAC_SFL_Scores_Farmer__r.socioe_bankAccount__c) = &apos;Si&apos;, 5, 0)
+ 
IF(OR(INCLUDES(LAC_SFL_Scores_Farmer__r.socioe_debtsFinancingSource__c, &quot;Crédito familiar&quot;)
,INCLUDES(LAC_SFL_Scores_Farmer__r.socioe_debtsFinancingSource__c, &quot;Prestamista&quot;)), 1, 0)
+ IF(INCLUDES(LAC_SFL_Scores_Farmer__r.socioe_debtsFinancingSource__c, &quot;Intermediario (comprador)&quot;), 3, 0)
+ IF(OR(INCLUDES(LAC_SFL_Scores_Farmer__r.socioe_debtsFinancingSource__c, &quot;Cooperativa de productores&quot;)
,INCLUDES(LAC_SFL_Scores_Farmer__r.socioe_debtsFinancingSource__c, &quot;Microfinanciera&quot;)
,INCLUDES(LAC_SFL_Scores_Farmer__r.socioe_debtsFinancingSource__c, &quot;Banco&quot;)
), 5, 0)
+
IF(LAC_SFL_Scores_Farm__r.fertilizerApplicationsLastYr__c &gt;= 3, 5, 
IF(LAC_SFL_Scores_Farm__r.fertilizerApplicationsLastYr__c = 2, 2,
IF(LAC_SFL_Scores_Farm__r.fertilizerApplicationsLastYr__c = 1, 1, 0  
)))
+
IF((services_UseInputsCreditTrainingArea__c * 100) &lt; 10, 0,
IF((services_UseInputsCreditTrainingArea__c * 100) &lt; 40, 1, 
IF((services_UseInputsCreditTrainingArea__c * 100) &lt; 60, 2,
IF((services_UseInputsCreditTrainingArea__c * 100) &lt; 80, 3,  
IF((services_UseInputsCreditTrainingArea__c * 100) &lt; 100, 4, 5
)))))
+
IF(INCLUDES(LAC_SFL_Scores_Farmer__r.service_meetingsParticipation__c, &quot;Participa en capacitaciones&quot;), 5, 0)
) / 2.5) * 0.5</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>services_UseInputsCreditTraining</label>
        <precision>18</precision>
        <required>false</required>
        <scale>2</scale>
        <trackTrending>false</trackTrending>
        <type>Number</type>
        <unique>false</unique>
