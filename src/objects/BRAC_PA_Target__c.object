<?xml version="1.0" encoding="UTF-8"?>
<CustomObject xmlns="http://soap.sforce.com/2006/04/metadata">
    <deploymentStatus>Deployed</deploymentStatus>
    <description>The target record for the BRAC PAs. These are set country wide at the moment hence the</description>
    <fields>
        <fullName>Date__c</fullName>
        <defaultValue>Today()</defaultValue>
        <description>The date that this target comes into affect</description>
        <inlineHelpText>Set this to be the date that the target starts. If left unset will default to today</inlineHelpText>
        <label>Start Date</label>
        <required>true</required>
        <type>Date</type>
    </fields>
    <fields>
        <fullName>Health_Visits__c</fullName>
        <description>The target for the number of health visits for this month</description>
        <inlineHelpText>Enter the number of health visits that the PA is supposed to make this month</inlineHelpText>
        <label>Target</label>
        <precision>18</precision>
        <required>true</required>
        <scale>0</scale>
        <type>Number</type>
    </fields>
    <fields>
        <fullName>Households_Visits__c</fullName>
        <description>Number of households the PA is supposed to visit in a month</description>
