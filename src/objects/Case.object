    <fields>
        <fullName>Case_Resolution_Time__c</fullName>
        <externalId>false</externalId>
        <formula>ABS(ROUND( 8 * (
   ( 5 * FLOOR( ( DATEVALUE(ClosedDate) - DATE( 1900, 1, 8) ) / 7) +
    MIN(5, 
     MOD( DATEVALUE(ClosedDate) - DATE( 1900, 1, 8), 7) +
     MIN( 1, 24 / 8 * ( MOD( ClosedDate - DATETIMEVALUE( &apos;1900-01-08 05:00:00&apos; ), 1 ) ) )
    ) 
   )
 -
   ( 5 * FLOOR( ( DATEVALUE( CreatedDate) - DATE( 1900, 1, 8) ) / 7) +
     MIN( 5,
      MOD( DATEVALUE( CreatedDate ) - DATE( 1996, 1, 1), 7 ) +
      MIN( 1, 24 / 8 * ( MOD( CreatedDate  - DATETIMEVALUE( &apos;1900-01-08 05:00:00&apos; ), 1) ) )
    )
   ) 
  ), 
0 ))</formula>
        <label>Case Resolution Time</label>
        <precision>18</precision>
        <required>false</required>
        <scale>2</scale>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Number</type>
        <unique>false</unique>
    </fields>
