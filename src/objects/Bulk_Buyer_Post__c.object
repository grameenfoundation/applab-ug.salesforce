    <deploymentStatus>Deployed</deploymentStatus>
    <description>A post that is made by a buyer or a seller on the bulk buyer system.</description>
    <enableActivities>false</enableActivities>
    <enableEnhancedLookup>false</enableEnhancedLookup>
    <enableFeeds>false</enableFeeds>
    <enableHistory>false</enableHistory>
    <enableReports>false</enableReports>
    <fields>
        <fullName>Active__c</fullName>
        <defaultValue>true</defaultValue>
        <description>Is this offer still available</description>
        <externalId>false</externalId>
        <label>Active</label>
        <type>Checkbox</type>
    </fields>
    <fields>
        <fullName>Commodity__c</fullName>
        <description>The commodity for sale</description>
        <externalId>false</externalId>
        <label>Commodity</label>
        <length>100</length>
        <required>true</required>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>End_Date__c</fullName>
        <defaultValue>Today() + 21</defaultValue>
        <description>The date that the offer ends</description>
        <externalId>false</externalId>
        <label>End Date</label>
        <required>false</required>
        <type>Date</type>
    </fields>
    <fields>
        <fullName>Poster__c</fullName>
        <deleteConstraint>SetNull</deleteConstraint>
