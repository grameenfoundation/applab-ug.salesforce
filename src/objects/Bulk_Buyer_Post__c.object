            </picklistValues>
            <picklistValues>
                <fullName>Terrible</fullName>
                <default>false</default>
            </picklistValues>
            <sorted>false</sorted>
        </picklist>
        <type>Picklist</type>
    </fields>
    <fields>
        <fullName>Quantity__c</fullName>
        <description>How much of the product is on offer</description>
        <externalId>false</externalId>
