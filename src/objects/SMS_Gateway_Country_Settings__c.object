<?xml version="1.0" encoding="UTF-8"?>
<CustomObject xmlns="http://soap.sforce.com/2006/04/metadata">
    <customSettingsType>List</customSettingsType>
    <customSettingsVisibility>Protected</customSettingsVisibility>
    <description>The settings for an SMS Gateway based on country</description>
    <enableFeeds>false</enableFeeds>
    <fields>
        <fullName>Class_Name__c</fullName>
        <description>The name of the class that contains the methods for sending messages through this gateway</description>
        <externalId>false</externalId>
        <label>Class Name</label>
        <length>100</length>
        <required>true</required>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Country_Code__c</fullName>
        <description>The dialling code for the country</description>
        <externalId>false</externalId>
        <label>Country Code</label>
        <precision>3</precision>
        <required>true</required>
        <scale>0</scale>
        <type>Number</type>
        <unique>true</unique>
    </fields>
    <fields>
        <fullName>ISO_Standard__c</fullName>
        <caseSensitive>false</caseSensitive>
        <description>The countries ISO standard short code</description>
        <externalId>false</externalId>
        <label>ISO Standard Code</label>
        <length>3</length>
        <required>true</required>
        <type>Text</type>
        <unique>true</unique>
    </fields>
    <label>Sms Gateway Country Settings</label>
</CustomObject>
