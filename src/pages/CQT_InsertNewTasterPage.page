<apex:page standardController="CQT_Taster__c" extensions="CQT_InsertNewTasterController" >
    <apex:form >
        <apex:sectionHeader title="Nuevo Catador" />
        <apex:pageBlock >
            <apex:pageBlockButtons location="Bottom">
                <apex:commandButton value="Grabar" action="{!save}" />
                <apex:commandButton value="Cancelar" action="{!cancel}" immediate="true"/>
            </apex:pageBlockButtons>
            <apex:pageBlockSection columns="1">
                <apex:inputField value="{!CQT_Taster__c.Name}"/>
                <apex:inputField value="{!CQT_Taster__c.firstName__c}"/>
                <apex:inputField value="{!CQT_Taster__c.lastName__c}"/>
                <apex:inputField value="{!CQT_Taster__c.phoneNumber__c}"/>
                <apex:inputField value="{!CQT_Taster__c.eMail__c}"/>
            </apex:pageBlockSection>
        </apex:pageBlock>
    </apex:form>
</apex:page>