<apex:page tabStyle="LAC_Farmer_Details__c" standardController="LAC_Farmer_Details__c" extensions="LacFarmerProfilePageController" language="{!PageLanguage}">
        <apex:pageBlock >
        <apex:pageBlockSection title="Productivity Profile" collapsible="false" columns="1">
        <apex:outputField value="{!CoffeeBaseline.Total_Farm_Area__c}"/>
        <apex:repeat value="{!cblLots}" var="cblLot">
            <apex:outputField value="{!cblLot.lotCoffeeVariety__c}"/>
        </apex:repeat>
        <apex:outputField value="{!CoffeeBaseline.Productivity__c }"/>
        <apex:outputField value="{!SignatureFarm.Production_last_year_Kg__c}"/>
        <apex:outputField value="{!CertificationFarm.farmCertifications__c}"/>
        </apex:pageBlockSection>
        <apex:pageBlockSection title="Commercialization" collapsible="false" columns="1">
        <apex:outputField value="{!CblCommercialization.producedCoffeeDestiny__c}"/>
        <apex:outputField value="{!CblCommercialization.lastHarvestPercCoffeeSoldToCoop__c}"/>
        <apex:outputField value="{!CblCommercialization.statusSoldCoffee__c}"/>
        <apex:outputField value="{!CblCommercialization.buysCoffee__c}"/>
        <apex:outputField value="{!SignatureFarm.coffeProcessingPlace__c}"/>
        </apex:pageBlockSection>
                <apex:pageBlockSection title="Socio Economic Information" columns="1" collapsible="false" >
            <apex:pageBlockSection title="Household" columns="1" collapsible="false">
           <apex:outputField value="{!SocioEconomic.personsInHousehold__c}"/>
           <apex:outputField value="{!SocioEconomic.homeOwnerShip__c}"/>
           <apex:outputField value="{!SocioEconomic.waterSupply__c}"/>
           <apex:outputField value="{!SocioEconomic.sewageDisposalSystem__c}"/>
           <apex:outputField value="{!SocioEconomic.electricEnergy__c}"/>
           </apex:pageBlockSection>
           <apex:pageBlockSection title="Education" columns="1" collapsible="false">
           <apex:outputField value="{!SocioEconomic.knowsHowToReadWrite__c}"/>
           <apex:outputField value="{!SocioEconomic.schoolingYears__c}"/>
           </apex:pageBlockSection>
           <apex:pageBlockSection title="Income" columns="1" collapsible="false">
           <apex:outputField value="{!SocioEconomic.householdIncomeSources__c}"/>
           <apex:outputField value="{!SocioEconomic.percentageOfIncomeCoffee__c}"/>
           <apex:outputField value="{!SocioEconomic.otherAgriculturalActivities__c}"/>
           </apex:pageblockSection>
           <apex:pageBlockSection title="Access to Financial Services" columns="1" collapsible="false">
           <apex:outputField value="{!SocioEconomic.savingsWay__c}"/>
           <apex:outputField value="{!SocioEconomic.bankAccount__c}"/>
           <apex:outputField value="{!SocioEconomic.debts__c}"/>
           <apex:outputField value="{!SocioEconomic.debtsFinancingSource__c}"/>
           <apex:outputField value="{!SocioEconomic.creditDestination__c}"/>
           </apex:pageBlockSection>
        </apex:pageBlockSection>
        <apex:pageBlockSection columns="1" title="Minority Population Group" collapsible="false">
           <apex:outputField value="{!SocioEconomic.populationGroup__c}"/>
           <apex:outputField value="{!SocioEconomic.socialSecutiryProgram__c}"/>
        </apex:pageBlockSection>
        <apex:pageBlockSection title="Farms" collapsible="false">
        <apex:repeat value="{!CblFarms}" var="cblFarm">
            <apex:outputLink value="/{!cblFarm.id}" target="top">{!cblFarm.farmName__c }</apex:outputLink>
        </apex:repeat>
        </apex:pageBlockSection>
        </apex:pageBlock>
</apex:page>