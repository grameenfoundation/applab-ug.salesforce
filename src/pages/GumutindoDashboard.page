                   <a href='{!$Page.GumutindoDashboardNew}?quarter={!quarter}&sfdc.tabName={!tabName}&editMode=true'>Update Metrics</a>
              </div>
              <div style="display:inline;padding-left:10px;visibility:{!IF(isEditMode == 'true','visible','hidden')}">
                  You are currently in Update Mode: <a href='{!$Page.GumutindoDashboardNew}?quarter={!quarter}&sfdc.tabName={!tabName}&editMode=false'>Leave Update Mode</a>
              </div>
             <!-- <apex:outputText rendered="{!isCurrentQuarter}" ></apex:outputText> -->
