<apex:page standardController="EMP_Organization_Member__c" extensions="EMPReplicantProfile" sidebar="false" language="es">
<apex:form >
    <apex:pageBlock >
      <apex:pageBlockSection title="Visitas Asistencia Técnica">
        <apex:pageBlockTable value="{!technicalAssistance}" var="fTA">
        	<apex:column headerValue="Fecha Visita">
        	<apex:outputLink target="_blank" value="/{!fTA.Id}">{!fTA.Date__c}</apex:outputLink>
        	</apex:column>
          	<apex:column headerValue="Número Visita" value="{!fTA.Visit_Number__c}"/>
          	<apex:column headerValue="Visita a" value="{!fTA.Visit_To__c}"/>
          	<apex:column headerValue="Consultor que realiza la visita" value="{!fTA.EMP_Technical_Assistance_Consultant__c}"/>
          	<apex:column headerValue="Recomendaciones" value="{!fTA.Recommendations_Next_Visit__c}"/>
        </apex:pageBlockTable>
      </apex:pageBlockSection>
    </apex:pageBlock>
</apex:form>
</apex:page>